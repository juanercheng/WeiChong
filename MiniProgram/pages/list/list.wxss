/* pages/list/list.wxss */

.warper ul li {
  list-style: none;
  display: flex;
  padding: 10px 15px;
  border-bottom: 1px solid #ddd;
}

.warper ul li .imgBox {
  margin-top: 10px;
  width: 80px;
  min-width: 80px;
  height: 80px;
  overflow: hidden;
  margin-right: 10px;
}

.warper ul li .imgBox image {
  width: 100%;
  height: 100%;
}

.warper ul li .content {
  height: 75px;
  overflow: hidden;
  display: inline-flex;
  flex-direction: column;
  padding: 10px 15px;
}

.warper ul li .content .title {
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
  font-size: 15px;
}

.warper ul li .content .small {
  margin: 10px 0;
  font-size: 13px;
  color: rgb(155, 155, 155);
}

.warper ul li .content .address {
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
  color: rgb(155, 155, 155);
}

.warper ul li .content  .positionText {
  font-size: 13px;
  word-wrap: nowrap;
  color: rgb(155, 155, 155);
}

.warper ul li .content .position {
  width: 14px;
  height: 14px;
}
.firstBar image {
  width: 55px;
  height: 55px;
  position: fixed;
} 

/**index.wxss**/

#Map {
  /* background: red; */
  position: fixed;
  width: 100%;
  height: 100%;
  left: 0;
  top: 0;
  right: 0;
  bottom: 0;
  z-index: 1;
  /* z-index: 1; */
}

.chargeInfomation {
  background: white;
  width: 250px;
  height: 140px;
  margin: 45% auto;
  border-radius: 10%;
  border: rgba(0, 0, 0, 0.4) solid 2px;
}

.chargeImgModel {
  width: 100%;
  height: 100px;
  margin: 0 auto;
}

.chargeImgModel cover-image {
  width: 100%;
  height: 100px;
}

.chargeAdress {
  /* background: black; */
  width: 100%;
  height: 70px;
  margin: 0 auto;
  text-align: left;
  line-height: 500%;
  font-size: 12px;
  position: fixed;
  text-decoration: underline;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
}

.chargeName {
  /* background: blue; */
  width: 100%;
  height: 70px;
  margin: 0 auto;
  text-align: left;
  line-height: 594%;
  font-size: 12px;
  border-top: 1px solid #0ca6ba;
  border-bottom: 1px solid #0ca6ba;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
}

.chargeAdress cover-image {
  width: 12px;
  height: 34px;
  /* margin-left: 20%;
 */
  position: absolute;
  left: 59%;
  top: 25%;
}

.scroll {
  height: 100%;
}
 .loading{
   width: 100%;
   height: 20px;
  font-size: 16px;
  text-align: center;
} 
.weui-loading {
  margin: 0 5px;
  width: 20px;
  height: 20px;
  display: inline-block;
  vertical-align: middle;
  -webkit-animation: weuiLoading 1s steps(12, end) infinite;
  animation: weuiLoading 1s steps(12, end) infinite;
  background: transparent url(data:image/svg+xml;base64,) no-repeat;
  background-size: 100%;
}
.weui-loadmore {
  width: 65%;
  margin: 1em auto;
  line-height: 1em;
  font-size: 14px;
  text-align: center;
}
.weui-loadmore__tips {
  display: inline-block;
  vertical-align: middle;
}